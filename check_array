class Solution:
    # Function to check if two arrays are equal or not.
    def check(self, arr1, arr2) -> bool:
        #code here
        arr1=sorted(arr1)
        arr2=sorted(arr2)
        
        if len(arr1) != len(arr2):
            return False
            
        for i in range(len(arr1)):
            if arr1[i] != arr2[i]:
                return False
        
        return True
